{
  "extends": "airbnb-base",
  "rules": {
    "comma-dangle": [2, "always-multiline"],
    "no-shadow": 0,
    "no-param-reassign": 0,
    "vars-on-top": 0,
    "padded-blocks": 0,
    "space-before-function-paren": 0,
    "func-names": 0,
    "no-nested-ternary": 0,
    "no-unneeded-ternary": 1,
    "prefer-template": 1,
    "max-len": 0,
    "arrow-body-style": 1,
    "new-cap": 1,
    "no-underscore-dangle": 0,
    "global-require": 0,
    "no-plusplus": 1,
    "class-methods-use-this": 1
  }
}
